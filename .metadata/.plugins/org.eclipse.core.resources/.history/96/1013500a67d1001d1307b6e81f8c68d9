package edu.fa;

import java.io.FileReader;
import java.io.FileWriter;

public class CharacterStreamSample {
	public static void main(String[] args) {
		FileReader fr = null;
		FileWriter fw = null;
		
		fr = new FileReader("D:/data/input.txt");
		fw = new FileWriter("D:/data/output.txt");
	}
}
