package model;

public class Student {

	private String name;
	private int grade;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getGrade() {
		return this.grade;
	}

	public void setGrade(int grade) {
		this.grade = grade;
	}
	
	public Student (String name, int grade) {
		this.name = name;
		this.grade = grade;
	}
}
