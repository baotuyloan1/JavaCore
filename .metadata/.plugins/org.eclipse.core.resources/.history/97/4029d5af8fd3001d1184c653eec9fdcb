public class Interruption {

}


class BankingThread extends Thread{
	
	@Override
	public void run() {
		int count = 0;
		while(count++ < 5) {
			try {
				Thread.sleep(1000);
			} catch (Exception e) {
				e.printStackTrace();
			}
		}
	}
}