public class ThreadSample {
	public static void main(String[] args) {
		CustomerThread ct = new CustomerThread();
		ct.start();
	}
}

class CustomerThread extends Thread {
	@Override
	public void run() {
		System.out.println("Customer Thread is running");
	}
}
